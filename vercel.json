{
  "build": {
    "cwd": ".",
    "installCommand": "npm install",
    "outputDirectory": null,
    "buildCommand": "npm run build --workspace=api",
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/packages/api/api/index.js"
    }
  ]
}
