# ============================================
# WEB DASHBOARD - Next.js .gitignore
# ============================================

# Dependencies
/node_modules
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# Next.js
/.next/
/out/
/build
.turbopack/

# Environment variables (CRITICAL - Never commit these)
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
.env*.local
*.env
!.env.example

# Testing
/coverage
*.lcov

# Misc
.DS_Store
*.pem

# Debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# Vercel
.vercel

# TypeScript
*.tsbuildinfo
next-env.d.ts

# VS Code (keep some configs)
.vscode/*
!.vscode/extensions.json
!.vscode/settings.json

# Supabase
.supabase/
